# $ cat ~/.env

WEB_PORT=80
DJANGO_ADMIN_PORT=8000
SERVER_IP=127.0.0.1 # The host machine IP


# MQ
RABBITMQ_DEFAULT_USER=rabbitMQ_username
RABBITMQ_DEFAULT_PASS=rabbitMQ_password
